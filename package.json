{
  "name": "centauri-tac",
  "version": "0.9.0",
  "description": "Tactical Card Game",
  "main": "./server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dshook/centauri-tac.git"
  },
  "scripts": {
    "dev": "grunt ct app:portal --dev=true",
    "dev-server": "npm run migrate && nodemon ./server",
    "start": "node ./server",
    "build": "grunt ct app:portal",
    "test": "node tests",
    "create-migration": "db-migrate create --sql-file",
    "migrate": "db-migrate up",
    "migrate-down": "db-migrate down -c 1",
    "prestart": "npm run migrate && npm run build"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "ng-table": "./node_modules/ng-table/dist/ng-table.js"
  },
  "browserify-shim": {
    "angular": "angular"
  },
  "browserify": {
    "transform": [
      "partialify"
    ]
  },
  "contributors": [
    "Dillon Shook <dshook@alumni.nmt.edu> (http://dillonshook.com)",
    "Brandon Valosek <bvalosek@gmail.com> (https://bvalosek.com)"
  ],
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/dshook/centauri-tac/issues"
  },
  "homepage": "https://github.com/dshook/centauri-tac#readme",
  "dependencies": {
    "aliasify": "^1.7.2",
    "angular": "^1.4.5",
    "angular-cookies": "^1.4.5",
    "angular-moment": "^0.10.3",
    "angular-resource": "^1.4.5",
    "angular-ui-bootstrap": "^0.13.4",
    "angular-ui-router": "^0.2.15",
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "bcrypt": "^0.8.5",
    "billy": "^1.7.0",
    "bluebird": "^2.9.34",
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.5",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "compression": "^1.5.2",
    "cookie-parser": "^1.3.5",
    "core-decorators": "^0.2.0",
    "cors": "^2.7.1",
    "db-migrate": "^0.9.20",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-jwt": "^3.1.0",
    "font-awesome": "^4.4.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.1",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^17.1.0",
    "jsonwebtoken": "^5.0.5",
    "load-grunt-tasks": "^3.2.0",
    "lodash": "^3.10.1",
    "loglevel": "^1.4.0",
    "moment": "^2.10.6",
    "ng-table": "^0.5.4",
    "nodemon": "^1.4.1",
    "partialify": "^3.1.5",
    "pg": "^4.4.1",
    "present": "0.0.6",
    "request": "^2.61.0",
    "sql-params": "^1.0.1",
    "tape": "^4.2.0",
    "ws": "^0.8.0"
  }
}
